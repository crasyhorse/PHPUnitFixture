{
    "name": "crasyhorse/phpunit-fixture",
    "version": "0.3.2",
    "description": "Adds a fixture command to PHP-Unit tests like the one in Cypress.",
    "keywords": ["Laravel","Fixture","JSON","Testing"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Florian Weidinger",
        "email": "CrasyHorse@gmx.net"
    }],
    "autoload": {
        "psr-4": {
            "CrasyHorse\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrasyHorse\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/crasyhorse/PHPUnitFixture.git"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "adbario/php-dot-notation": "2.x-dev",
        "fandogh/php-ubjson": "^0.2",
        "pcrov/jsonreader": "dev-master",
        "opis/json-schema": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "vimeo/psalm": "^4.8",
        "league/flysystem-memory": "^1.0",
        "friendsofphp/php-cs-fixer": "3.4.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "test-g": "vendor/bin/phpunit --group",
        "psalm": "vendor/bin/psalm",
        "psalm-i": "vendor/bin/psalm --show-info=true"
    },
    "minimum-stability": "dev"
}
